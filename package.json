{
  "name": "autonomous-minecraft-bot",
  "version": "4.1.0",
  "description": "Enterprise-grade autonomous Minecraft bot with ML engine, React dashboard, multi-bot coordination, and advanced analytics",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd frontend && npm install",
    "prestart": "npm run frontend:build",
    "start": "node index.js",
    "dev": "node index.js",
    "dashboard": "node src/dashboard.js",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "train-models": "node scripts/train-models.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "validate": "node -c index.js && node -c src/*.js && echo '✓ All files validated'",
    "check-env": "node -e \"require('dotenv').config(); console.log('✓ Environment loaded'); console.log('Host:', process.env.MINECRAFT_HOST); console.log('Telegram:', process.env.TELEGRAM_BOT_TOKEN ? 'Configured' : 'Not configured'); console.log('Gemini AI:', process.env.GEMINI_API_KEY ? 'Configured' : 'Not configured');\""
  },
  "keywords": [
    "minecraft",
    "bot",
    "mineflayer",
    "autonomous",
    "telegram",
    "ai",
    "gemini",
    "machine-learning",
    "tensorflow",
    "react",
    "multi-bot",
    "coordination",
    "analytics",
    "protection",
    "optimization",
    "hybrid-intelligence",
    "self-healing",
    "enterprise",
    "dashboard",
    "plugin-system"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "*",
    "@tensorflow/tfjs": "^4.15.0",
    "better-sqlite3": "*",
    "cors": "*",
    "dotenv": "*",
    "express": "*",
    "mineflayer": "*",
    "mineflayer-collectblock": "*",
    "mineflayer-pathfinder": "*",
    "node-telegram-bot-api": "*",
    "socket.io": "^4.6.0",
    "winston": "*",
    "ws": "*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0"
  }
}
